#include "fsm_manual.h"
#include "i2c-lcd.h"
#include "led_display.h"
#include <string.h>
#include <stdio.h>
void show4();
void fsm_manual(){
	if (state == MANUAL) {
		show2();
	}
		switch () {
				case RED_GREEN:
on_red1_led();
on_green2_led();
					break;
				case GREEN_RED:
					on_red2_led();
					on_green1_led();
										break;
	}
}

void show2(){
	lcd_goto_XY(1,0);
	lcd_send_string("       ");
	lcd_goto_XY(2, 0);
		lcd_send_string("    ");
		lcd_goto_XY(2, 4);
				lcd_send_string("MANUAL");
}
